<?xml version="1.0"?>

<project name="Aplaster.Setup" xmlns="http://nant.sf.net/release/0.85/nant.xsd">
	<target name="all" depends="compile"/>

	<target name="clean">
		<delete>
			<fileset>
				<include name="setup.exe"/>
			</fileset>
		</delete>
	</target>

	<target name="compile">
		<iscc project="setup.iss"/>
	</target>
</project>
